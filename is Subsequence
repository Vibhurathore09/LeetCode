class Solution {
    public boolean isSubsequence(String s, String t) {
        int j = 0 ;
        for(int i = 0 ; j < s.length() && i < t.length() ; i++){
            if(s.charAt(j) == t.charAt(i)){
                j++;
            }
        }
        // System.out.println(c);
        if(j == s.length())
        {
            return true;
        }else{
            return false;
        }
    }
}
